  <%= form_tag admin_path, method: :get, class: "form-inline mr-auto" do %>
    <%= text_field_tag query_model_name, params[query_model_name], class: "form-control mr-3", placeholder: t(placeholder_model_name) %>
    <%= button_tag type: :submit, class: "btn btn-primary mx-1" do %>
      <%= icon('fas','search') %> Chercher
    <% end %>
    <%= button_tag type: :reset, class: "btn btn-secondary mx-1" do %>
      <%= icon('fa','times') %> annuler
    <% end %>
  <% end %>
